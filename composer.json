{
    "name": "samwilson/taggedfiles",
    "description": "A web-based file management system. Supports tagging, access control, and revision history.",
    "license": "GPL-3.0+",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "twig/twig": "^2.1",
        "erusev/parsedown": "^1.0",
        "league/flysystem": "^1.0",
        "league/route": "^2.0",
        "eternicode/bootstrap-datepicker": "^1.0",
        "intervention/image": "^2.0",
        "swiftmailer/swiftmailer": "^5.0",
        "moxiecode/plupload": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/var-dumper": "^3.0",
        "squizlabs/php_codesniffer": "^2.0"
    },
    "config": {
        "component-dir": "assets/components"
    }
}
