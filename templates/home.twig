{% extends "base.twig" %}

{% block content %}
    <div class=" row">
        {% for item in items %}
        <div class="col-lg-2">
            <a href="{{baseurl}}/{{item.id}}" class="thumbnail">
                <h2>{{item.title|raw}}</h2>
                {{item.getDescription}}
            </a>
        </div>
        {% endfor %}
    </div>
{% endblock %}